cd figdata4
load FigData4SigP
Tau=Tau*1000;

subplot(331)
plot(JitterData1.Tau,JitterData1.Rmodel/1000,'color',[0.75 .75 .75],'linewidth',4)
hold on
plot(JitterData1.Tau,JitterData1.Rnoise/1000,'k')
axis([-10 10 -1 20])
set(gca,'YTick',[0 10 20])

