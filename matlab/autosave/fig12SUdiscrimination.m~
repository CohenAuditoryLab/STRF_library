subplot(222)
plot(log10(Sig(4:10)),mean(D3(4:10,8,:),3),'r')
hold on
errorbar(log10(Sig(4:10)),mean(D3(4:10,8,:),3),std(D3(4:10,8,:),[],3)/sqrt(20),'r','linewidth',2)
set(gca,'XTick',(Sig(3:2:end)))
set(gca,'XTickLabel',[Sig(3:2:end)])
xlim(([Sig(3)*.8 Sig(end)/.8]))
set(gca,'Box','off')

