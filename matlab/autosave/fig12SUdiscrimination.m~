


subplot(221)
plot(log10(Tau),mean(D3(4,:,:),3),'r')
hold on
errorbar(log10(Tau),mean(D3(4,:,:),3),std(D3(4,:,:),[],3)/sqrt(20),'r','linewidth',2)
hold on
plot(log10(Tau),mean(D3(10,:,:),3),'g')
hold on
errorbar(log10(Tau),mean(D3(10,:,:),3),std(D3(10,:,:),[],3)/sqrt(20),'g','linewidth',2)
set(gca,'XTick',log10(Tau(1:2:end)))
set(gca,'XTickLabel',([Tau(1:2:end)]))
xlim(log10([0.1*.8 3.2/0.8]))
set(gca,'Box','off')
